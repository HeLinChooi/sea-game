  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Launch Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "Launch Main",
      "request": "launch",
      "mainClass": "SeaDesignPattern/application.Main",
      "projectName": "SeaDesignPattern"
    },
    {
      "type": "java",
      "name": "Launch ShippingDockManager",
      "request": "launch",
      "mainClass": "SeaDesignPattern/application.logic.ShippingDockManager",
      "projectName": "SeaDesignPattern",
      "vmArgs": "--module-path \"D:/Program or SDK/javafx-sdk-19/lib\" --add-modules javafx.controls,javafx.fxml"
    }
  ]
}